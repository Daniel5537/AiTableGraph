{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": false,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "downlevelIteration": true,
        "lib": ["es5", "es7", "es6", "dom"]
    },
    "include": [
        "./src/**/*"
    ],
    "exclude": [
        "./src/graphic/*",
        "./src/test/*"
    ]
}
